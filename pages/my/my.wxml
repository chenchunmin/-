<view class="my">
  <view class="head">
    <van-image width="100%" height="100%" src="/assets/coupon/1.jpg" />
  </view>
  <text class="name">{{name}}</text>
  <text class="code">{{code}}</text>
</view>

<view class="order">
  <text class="goods">我的订单</text>
  <view class="whole" catchtap="entire">
    <text class="text">全部订单</text>
    <image src="/assets/icon/right.png"></image>
  </view>
  <view class="list">
    <view class="pay" bindtap="Orderinfo" data-index="0">
      <van-icon size="60rpx" name="/assets/icon/fk.png" info="1" />
    </view>
    <view class="pay" bindtap="Orderinfo" data-index="1">
      <van-icon size="60rpx" name="/assets/icon/fh.png" info="99+" />
    </view>
    <view class="pay" bindtap="Orderinfo" data-index="2">
      <van-icon size="60rpx" name="/assets/icon/sh.png" info="9"/>
    </view>
    <view class="pay" bindtap="Orderinfo" data-index="3">
      <van-icon size="60rpx" name="/assets/icon/pj.png" info="9" />
    </view>
    <view class="goodstext">
      <text>全部</text>
      <text>待支付</text>
      <text>已支付</text>
      <text>已完成</text>
    </view>
  </view>
</view>

<view class="classify">
  <view class="tet" catchtap="skip">
    <image class="notify" src="/assets/icon/dz.png"></image>
    <text>我的地址</text>
    <image class="image" src="/assets/icon/right.png"></image>
  </view>
  <view class="tet" catchtap="mynotify">
    <image class="notify" src="/assets/icon/yhq.png"></image>
    <text>我的优惠券</text>
    <image class="image" style="left: 420rpx;" src="/assets/icon/right.png"></image>
  </view>
  <view class="tet" catchtap="activity"> 
    <image class="notify" src="/assets/icon/hd.png"></image>
    <text>活动中心</text>
    <image class="image" src="/assets/icon/right.png"></image>
  </view>
  <view class="tet">
    <image class="notify" src="/assets/icon/kef.png"></image>
    <text>联系客服</text>
    <image class="image" src="/assets/icon/right.png"></image>
  </view>
  <view class="tet" catchtap="tet">
    <image class="notify" src="/assets/icon/lc.png"></image>
    <text>转运流程</text>
    <image class="image" src="/assets/icon/right.png"></image>
  </view>
  <view class="tet" catchtap="notify">
    <image class="notify" src="/assets/icon/xz.png"></image>
    <text>转运须知</text>
    <image class="image" src="/assets/icon/right.png"></image>
  </view>
</view>